@model List<Student>

    <table class="table table-bordered  table-hover">
          <tr>
            <td>Id</td>
            <td>ImageUrl</td>           
            <td>Name</td>
       </tr>
    
    @foreach(var item in Model)
    {
        <tr>
        
            <td>@item.Id</td>
            <td>@item.ImageUrl</td>
            <td>@item.Name</td>
        </tr>
   
    }
  

    </table>


